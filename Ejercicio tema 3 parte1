public class ejercicioParte1 {
    public static void main(String[] args) {
    int valor1=10;
    int valor2=20;
    int valor3=10;
    var resultado=suma(valor1,valor2,valor3);
        System.out.println("El valor de la suma es "+resultado);

    }
    public static int suma(int a,int b,int c){
        return a+b+c;
    }
}
